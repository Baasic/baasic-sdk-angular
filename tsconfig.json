{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "baseUrl": ".",
        "typeRoots": [],
        "paths": {
            "*": [
                "*",
                "src/*",
                "dependencies/baasic-sdk-javascript/build/lib/types/*"
            ]
        },
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "importHelpers": true,
        "noEmitHelpers": true,
        "lib": [
            "dom",
            "es6"
        ]

    },
    "exclude": [
        "node_modules/",
        "build/",
        "dependencies/baasic-sdk-javascript/",
        "dependencies/baasic-sdk-javascript/node_modules/"
    ]
}